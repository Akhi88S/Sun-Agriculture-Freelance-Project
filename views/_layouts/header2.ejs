        <button
          class="navbar-toggler"
          type="button"
          data-toggle="collapse"
          data-target="#navbarsExampleDefault"
          aria-controls="navbarsExampleDefault"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div id="navbarsExampleDefault">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item">
              <a href="/cart/checkout" class="checkout"
                ><i class="icon ion-md-cart"></i>( <% if(typeof cart !=
                "undefined") { %> <%= cart.length %> <% } else { %> 0 <% } %>
                )</a
              >
            </li>
          </ul>
        </div>
        <div class="collapse navbar-collapse" id="navbarsExampleDefault">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item dropdown">
              <a
                class="nav-link dropdown-toggle"
                href="#"
                id="navbarDropdown"
                role="button"
                data-toggle="dropdown"
                aria-haspopup="true"
                aria-expanded="false"
              >
                Categories
              </a>
              <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                <a class="dropdown-item" href="/products">All products</a>
                <div class="dropdown-divider"></div>
                <% categories.forEach(function(c) { %>
                <a class="dropdown-item" href="/products/<%= c.slug %>">
                  <%= c.title %></a
                >
                <% }); %>
                <div class="dropdown-divider"></div>
                <a class="dropdown-item" href="/products">Hot Deals</a>
              </div>
            </li>
            <% pages.forEach(function(page) { %> <% if(page.slug == 'home') { %>
            <li class="nav-item">
              <a class="nav-link" href="/"> <%= page.title %></a>
            </li>
            <% } else if(page.slug == 'sales') { %>
            <li class="nav-item">
              <a class="nav-link sales" href="<%= page.slug %>">
                <%= page.title %></a
              >
            </li>
            <% } else { %>
            <li class="nav-item">
              <a class="nav-link" href="<%= page.slug %>"> <%= page.title %></a>
            </li>
            <% } %> <% }); %>
          </ul>
          <br />
          <form
            class="form-inline"
            onsubmit="fun()"
            action="/cart/search"
            method="post"
          >
            <input
              class="form-control mr-sm-2 search-input"
              name="search"
              type="search"
              placeholder="Search store"
              aria-label="Search"
            />
            <svg class="svg-icon search-icon nav-item" viewBox="0 0 20 20">
